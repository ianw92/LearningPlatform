<p id="notice"><%= notice %></p>

<h1>Tasks</h1>

<table>
  <thead>
    <tr>
      <th>Completed</th>
      <th>Todo List Name</th>
      <th>Title</th>
      <th>Due date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <%= render partial: "task",
               collection: @tasks %>
  </tbody>
</table>

<br>

<%= link_to 'New Task', new_task_path %>
